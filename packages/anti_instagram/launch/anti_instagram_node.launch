
<launch>
	<arg name="veh"  doc="Name of vehicle"/>

    <arg name="node_name" default="anti_instagram_node"/>
    <arg name="param_file_name" default="default" doc="Specify a param file" />
    <arg name="pkg_name" value="anti_instagram"/>
    <arg name="node_name" default="anti_instagram_node"/>

    <group ns="$(arg veh)">
        <node name="anti_instagram_node" pkg="anti_instagram" type="anti_instagram_node.py" output="screen">
            <rosparam command="load" file="$(find anti_instagram)/config/$(arg node_name)/$(arg param_file_name).yaml"/>
            <remap from="anti_instagram_node/uncorrected_image/compressed" to="camera_node/image/compressed"/>
        </node>

    </group>

</launch>
